{
  "name": "nashcash-wallet-backend",
  "version": "2.0.5",
  "description": "![image](https://user-images.githubusercontent.com/34389545/35821974-62e0e25c-0a70-11e8-87dd-2cfffeb6ed47.png)",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run-script build",
    "build": "tsc",
    "test": "npm run-script build && node dist/tests/tests.js",
    "test-all": "npm run-script build && node dist/tests/tests.js --do-performance-tests",
    "style": "tslint -c tslint.json 'lib/**/*.ts'",
    "docs": "typedoc --options ./typedoc.js ./lib/; touch docs/.nojekyll",
    "prepare": "npm run-script build"
  },
  "files": [
    "dist/lib/*"
  ],
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "lodash": "^4.17.15",
    "object-sizeof": "^1.6.0",
    "pbkdf2": "^3.0.17",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "nashcash-utils": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/pbkdf2": "^3.0.0",
    "@types/request-promise-native": "^1.0.17",
    "colors": "^1.4.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "tslint": "^6.1.1",
    "typedoc": "^0.17.4",
    "typedoc-plugin-no-inherit": "^1.1.10"
  },
  "lint-staged": {
    "**/*.{ts}": [
      "./node_modules/tslint/bin/tslint -c tslint.json --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nash-Cash/wallet-backend.git"
  },
  "bugs": {
    "url": "https://github.com/Nash-Cash/wallet-backend/issues"
  },
  "homepage": "https://github.com/Nash-Cash/wallet-backend#readme"
}
